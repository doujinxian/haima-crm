<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.haima.crm.dao.ComplaintContentDao">
	
	<resultMap id="RM_ComplaintContent" type="com.haima.crm.entity.ComplaintContent">
