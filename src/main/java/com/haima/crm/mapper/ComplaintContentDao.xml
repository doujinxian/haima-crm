	<update id="update" parameterType="com.haima.crm.entity.ComplaintContent">
		update t_complaint_content 
		<set>
			<if test="complainCode != null and complainCode != ''">
				`complain_code` = #{complainCode}, 			
			</if>
			<if test="complainContent != null and complainContent != ''">
				`complain_content` = #{complainContent}, 			
			</if>
			<if test="delFlag != null and delFlag != 0 ">
