	<!-- 用于select查询公用抽取的列 -->
	<sql id="ComplaintContent_columns">
	    <![CDATA[
			`id`, 			
			`complain_code`, 			
			`complain_content`, 			
			`del_flag`, 			
			`create_by`, 			
			`create_time`, 			
			`update_by`, 			
			`update_time`			
	    ]]>
	</sql>
	
	<!-- 用于where查询公用抽取的列 -->
	<sql id="ComplaintContent_condition">
		<where>
        <trim suffix="" suffixOverrides="and">
			<if test="complainCode != null and complainCode != ''">
				`complain_code` = #{complainCode} and 			
			</if>
			<if test="complainContent != null and complainContent != ''">
				`complain_content` = #{complainContent} and 			
			</if>
			<if test="delFlag != null and delFlag != 0 ">
				`del_flag` = #{delFlag} and 			
			</if>
			<if test="createBy != null and createBy != ''">
				`create_by` = #{createBy} and 			
			</if>
			<if test="createTime != null">
				`create_time` = #{createTime} and 			
			</if>
			<if test="updateBy != null and updateBy != ''">
				`update_by` = #{updateBy} and 			
			</if>
			<if test="updateTime != null">
				`update_time` = #{updateTime}			
			</if>
        </trim>
		</where>
	</sql>
	
	<select id="queryObject" resultMap="RM_ComplaintContent">
		select 
		<include refid="ComplaintContent_columns" />
		from t_complaint_content 
		where
		id = #{value}
	</select>

	<select id="queryList" resultMap="RM_ComplaintContent">
		select 
		<include refid="ComplaintContent_columns" />
		from t_complaint_content
		<include refid="ComplaintContent_condition"/> 
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="queryTotal" resultType="java.lang.Integer">
		select 
		count(1) 
		from t_complaint_content 
		<include refid="ComplaintContent_condition"/>
	</select>
	 
	<insert id="save" parameterType="com.haima.crm.entity.ComplaintContent" useGeneratedKeys="true" keyProperty="id">
		insert into t_complaint_content
		(
		<trim suffix="" suffixOverrides=",">
			<if test="complainCode!=null and complainCode != ''">
